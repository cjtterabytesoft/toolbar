{
    "name": "cjtterabytesoft/toolbar",
    "description": "Widget ToolBar Gridview. [Yii 3.0/Bootstrap4]",
    "keywords": ["yii3", "bootstrap4", "widget", "toolbar", "gridview"],
    "homepage": "https://github.com/cjtterabytesoft/toolbar",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cjtterabytesoft/toolbar/issues",
        "source": "https://github.com/cjtterabytesoft/toolbar/widgets",
        "forum":  "https://discuss.tusoporte.net"
    },
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "terabytefrelance@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~3.0@dev",
        "yiisoft/yii2-bootstrap4": "@dev",
        "dompdf/dompdf": "~0.8.2",
        "phpoffice/phpspreadsheet": "~1.3.1",
        "phpoffice/phpword": "~0.15.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "@dev",
        "yiisoft/yii2-gii": "@dev",
        "phpunit/phpunit": "~7.0.2"        
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"cjtterabytesoft\\widgets\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"cjtterabytesoft\\widgets\\tests\\": "tests"}
    },
    "config": {
        "process-timeout": 1800
    }
}
